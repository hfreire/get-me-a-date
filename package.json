    "@types/lodash": "4.14.64",
    "angular-in-memory-web-api": "0.3.2",
    "angular2-moment": "1.3.3",
    "core-js": "2.4.1",
    "font-awesome": "4.7.0",
    "rxjs": "5.0.1",
    "systemjs": "0.20.13",
    "zone.js": "0.8.11",
    "aws-sdk": "2.60.0",
    "bluebird": "3.5.0",
    "bluebird-retry": "0.10.1",
    "brakes": "2.5.3",
    "health-checkup": "1.0.8",
    "lodash": "4.17.4",
    "modern-logger": "1.3.8",
    "nightmare": "2.10.0",
    "random-http-useragent": "1.1.0",
    "request": "2.81.0",
    "serverful": "1.0.29",
    "sqlite3": "3.1.8",
