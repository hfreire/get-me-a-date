    "@angular/compiler": "4.2.6",
    "@angular/core": "4.3.0",
    "@angular/forms": "4.2.6",
    "@angular/http": "4.3.0",
    "@angular/material": "2.0.0-beta.8",
    "@angular/platform-browser": "4.2.6",
    "@angular/platform-browser-dynamic": "4.3.0",
    "@angular/router": "4.2.6",
    "@types/lodash": "4.14.68",
    "angular-in-memory-web-api": "0.3.2",
    "angular2-moment": "1.5.0",
    "aws-sdk": "2.85.0",
    "bluebird": "3.5.0",
    "bluebird-retry": "0.11.0",
    "brakes": "2.5.3",
    "chart.js": "2.6.0",
    "core-js": "2.4.1",
    "facebook-login-for-robots": "1.0.1",
    "font-awesome": "4.7.0",
    "hammerjs": "2.0.8",
    "happn-wrapper": "1.0.5",
    "health-checkup": "1.0.8",
    "lodash": "4.17.4",
    "modern-logger": "1.3.15",
    "moment": "2.18.1",
    "ng2-charts": "1.6.0",
    "request": "2.81.0",
    "rxjs": "5.4.2",
    "sequelize": "4.2.1",
    "serverful": "1.1.5",
    "sharp": "0.18.1",
    "sqlite3": "3.1.8",
    "systemjs": "0.20.15",
    "tinder-wrapper": "1.0.3",
    "zone.js": "0.8.13"
  },
  "devDependencies": {
    "chai": "4.1.0",
    "chai-as-promised": "7.1.1",
    "coveralls": "2.13.1",
    "eslint": "4.1.1",
    "eslint-config-hfreire": "1.0.6",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-json": "1.2.0",
    "eslint-plugin-mocha": "4.11.0",
    "eslint-plugin-node": "5.1.0",
    "eslint-plugin-promise": "3.5.0",
    "eslint-plugin-standard": "3.0.1",
    "eslint-plugin-unicorn": "2.1.2",
    "istanbul": "0.4.5",
    "last-release-github": "1.1.1",
    "mocha": "3.4.2",
    "node-sass": "4.5.3",
    "pre-git": "3.14.1",
    "semantic-release": "6.3.6",
    "testdouble": "3.2.2",
    "tslint": "5.5.0",
    "tslint-config-standard": "6.0.1",
    "typescript": "2.4.1"
  },
  "config": {
    "pre-git": {
      "commit-msg": "conventional"
    }
  },
  "release": {
    "getLastRelease": "last-release-github"
  },
  "scripts": {
    "commit": "./node_modules/.bin/commit-wizard",
    "clean": "rm -rf coverage && find src/web -type f -name '*.js.map' -exec rm {} + && find src/web -type f -name '*.js' -exec rm {} +",
    "lint": "node_modules/.bin/eslint . --ext .json --ext .js && node_modules/.bin/tslint 'src/web/**/*.ts'",
    "coverage": "node_modules/.bin/istanbul cover --include-all-sources --root src --print detail ./node_modules/mocha/bin/_mocha -- --recursive test",
    "test": "npm run clean && npm run lint && npm run coverage",
    "compile:ts": "./node_modules/.bin/tsc",
    "compile:sass": "./node_modules/.bin/node-sass -o src/web/assets/styles/ src/web/assets/styles/styles.scss",
    "compile": "npm run clean && npm run compile:ts && npm run compile:sass",
    "watch:sass": "./node_modules/.bin/node-sass -w -o src/web/assets/styles/ src/web/assets/styles/styles.scss",
    "semantic-release": "./node_modules/.bin/semantic-release pre && node_modules/.bin/semantic-release post",
    "build": "pushd ./share/docker; NAME=$npm_package_name ./hooks/build; popd",
    "push": "pushd ./share/docker; NAME=$npm_package_name ./hooks/push; popd",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/.bin/coveralls",
    "start": "node src/app.js"
  }
}
